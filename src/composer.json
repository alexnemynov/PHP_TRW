{
    "require": {
        "ramsey/uuid": "^4.7",
        "vlucas/phpdotenv": "^5.6",
        "psr/container": "^2.0",
        "symfony/mailer": "^7.1",
        "doctrine/dbal": "^4.2",
        "doctrine/orm": "^3.3",
        "symfony/cache": "^7.1",
        "doctrine/migrations": "^3.8",
        "illuminate/database": "^11.29",
        "illuminate/events": "^11.29",
        "laravel/serializable-closure": "^1.3",
        "ext-curl": "*",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    }
}
